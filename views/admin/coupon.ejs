<div class="container-fluid"> 
    <h1>coupon List</h1>  
<!-- Button trigger modal -->
<div class="d-flex justify-content-center">
    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal">
      Add Coupon
    </button>
  </div>
  
  
  <!-- Modal -->
  <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <form action="/addCoupon" method="post">
                <div class="modal-header">
                    <h2 class="modal-title" id="exampleModalLabel">add coupon</h2>
                    <button type="button" class="close " data-dismiss="modal" aria-label="Close" style="font-size: 5rem;">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="form-row">
                        <label for="couponCode" class="form-label">Coupon Code</label>
                        <input type="text" class="form-control" id="couponCode" name="couponCode" required>
                    </div>
                    <div class="form-row" >
                        <label class="form-label">Coupon Status</label>
                        <div class="form-check">
                            <input class="form-chec
                            k-input" type="radio" name="couponStatus" id="activeStatus" value="active" required>
                            <label class="form-check-label" for="activeStatus">Active</label>
                        </div>
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="couponStatus" id="inactiveStatus" value="inactive" required>
                            <label class="form-check-label" for="inactiveStatus">Inactive</label>
                        </div>
                    </div>

                    <div class="form-row" >
                        <label class="form-label">Coupon Type</label>
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="couponType" id="fixedAmount" value="fixedAmount" required>
                            <label class="form-check-label" for="fixedAmount">FixedAmount</label>
                        </div>
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="couponType" id="percentage" value="percentage" required>
                            <label class="form-check-label" for="percentage">Percentage</label>
                        </div>
                    </div>

                    <div class="form-row">
                        <label for="discountValue" class="form-label">Discount Value</label>
                        <input type="number" class="form-control" id="discountValue" name="discountValue" required>
                    </div>

                    <div class="form-row">
                        <label for="endDate" class="form-label">End Date</label>
                        <input type="date" class="form-control" id="endDate" name="endDate" required>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-primary">Add Coupon</button>
                </div>
            </form>
        </div>
    </div>
</div>



    <table id="couponTable" class="table table-striped">
        <thead>
            <tr>
                <th>coupon code</th>
                <th>coupon type</th>
                <th>discount value</th>
                <th>end date</th>
                <th>status</th>
                <th>action</th>
            </tr>
        </thead>
        <tbody>
            <% coupon.forEach(coupon => { %>
                <tr>
                    <td><%= coupon.couponCode %></td>
                    <td><%= coupon.couponType %></td>
                    <td><%= coupon.discountValue %></td>
                    <td><%= coupon.endDate %></td>
                    <td>
                        <span class="badge <%= coupon.couponStatus === 'active' ? 'bg-success' : 'bg-danger' %>">
                            <%= coupon.couponStatus %>
                        </span>
                    </td>
                                        <td> <a href="/editCoupon/<%= coupon._id %>" class="btn btn-primary btn-sm">
                        <i class="fas fa-edit"></i>
                        
                    </a>
                    <form action="/deleteCoupon/<%=coupon._id%>" method="post" style="display: inline;">
                       <button  type="submit" class="btn-danger btn-sm">  <i class="fas fa-trash-alt"></i>  </button>                        
                    
                </form>
                </td> 
                </tr>
            <% }) %>
            
        </tbody>
        
    </table>
    


   
