        <script>
        $(document).ready(function(){
            $('#couponTable').dataTable();
        });
        </script>
      <script>
        document.addEventListener("DOMContentLoaded", function() {
    let coupons = document.querySelectorAll("#couponTable tbody tr");
    coupons.forEach(coupon => {
        let endDateStr = coupon.querySelector("td:nth-child(4)").innerText;
        let endDate = new Date(endDateStr);
        let statusElement = coupon.querySelector("td:nth-child(5) span");

        if (endDate < new Date()) {
            statusElement.innerText = "inactive";
            statusElement.classList.remove("bg-success");
            statusElement.classList.add("bg-danger");
        } else {
            statusElement.innerText = "active";
            statusElement.classList.remove("bg-danger");
            statusElement.classList.add("bg-success");
        }
    });
});

    </script>

