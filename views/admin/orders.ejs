<div class="container-fluid"> 
<h1>order listing</h1>  
<!-- Button trigger modal -->
<div style="display: flex;justify-content: center;">
    <button id="showFormBtn">Sales Report</button>
    <form id="reportForm" action="/orderReport" method="post" style="display: none;">
        <label for="startDate">Start Date:</label>
        <input type="date" id="startDate" name="startDate">
        <label for="endDate">End Date:</label>
        <input type="date" id="endDate" name="endDate">
        <button type="button" onclick="getReport()">Get Report</button>
    </form>
</div>

<script src="https://cdn.jsdelivr.net/npm/axios@1.6.7/dist/axios.min.js"></script>

<script>

    function getReport(){
        let startDate=document.getElementById('startDate').value;
        let endDate=document.getElementById('endDate').value;

        axios.post('/orderReport',{
            startDate,
            endDate
        },
    {
     responseType:'blob'
    }).then(response=>{
            const url=window.URL.createObjectURL(new Blob([response.data]))
            const link=document.createElement('a')
            link.href=url;
            link.setAttribute('download','sales_report.xlsx')
            document.body.appendChild(link)
            link.click()
           console.log(response)
        }).catch(error=>{
            console.log(error)
        })

    }




    const showFormBtn = document.getElementById('showFormBtn');
    const reportForm = document.getElementById('reportForm');

    showFormBtn.addEventListener('click', () => {
        if (reportForm.style.display === 'none') {
            reportForm.style.display = 'block';
        } else {
            reportForm.style.display = 'none';
        }
    });

    
</script>


<table id="myTableCoupon" class="table table-striped">
    <thead>
        <tr>
            <th>order:ID</th>
            <th>productName</th>
            <th>price</th>
            <th>userName</th>
            <th>status</th>
            <th>status updation</th>
            <th>View</th>
        </tr>
    </thead>
    <tbody>
       <% usersWithOrders.forEach(user => { %>
        <% user.orders.forEach((orders, orderIndex) => { %>
            <% orders.products.forEach((product, productIndex) => { %>
          
            <tr>
                <td><%= orders._id %></td>
                <td><%= product.productName %></td>
                <td><%= product.productPrice %></td>
                <td><%= user.username %></td>
                <td class="
                    <% if (product.orderStatus === 'cancelled') { %>
                        text-danger
                    <% } else if (product.orderStatus === 'pending') { %>
                        text-warning
                    <% } else if (product.orderStatus === 'delivered') { %>
                        text-success
                    <% } else if(product.orderStatus==='shipped'){%>
                        text-primary
                    <%}%>"
                >
                    <%= product.orderStatus %>
                </td>
                <td>
                    <form action="" id="updateOrderForm-<%=orders._id%>-<%=orderIndex%>" method="post">
                        <select name="status" id="" class="form-select" onchange="updateOrderStatus(event,'<%=orders._id%>','<%=product.productId%>','<%=orderIndex%>')">
                            <% if(product.orderStatus!=='cancelled'){%>

                            
                            <option value="Pending" <%= product.orderStatus === "Pending" ? "selected" : "" %> class="<%= product.orderStatus === 'Pending' ? 'text-warning' : '' %>"> Pending</option>
                            <option value="delivered" <%= product.orderStatus === "delivered" ? "selected" : "" %> class="<%= product.orderStatus === 'delivered' ? 'text-success' : '' %>"> delivered</option>
                            <option value="shipped" <%= product.orderStatus === "shipped" ? "selected" : "" %> class="<%= product.orderStatus === 'shipped' ? 'text-primary' : '' %>"> shipped</option>
                            <%}%>
                            <% if (product.orderStatus !== 'delivered') { %>
                                <option value="cancelled" <%= product.orderStatus === "cancelled" ? "selected" : "" %> class="<%= product.orderStatus === 'cancelled' ? 'text-danger' : '' %>"> cancelled</option>
                            <% } %>
                            
                        </select>
                    </form>
                    
                    
                </td>
                
                
                <td>
                    <button type="button" class="btn btn-primary btn-sm" data-bs-toggle="modal" data-bs-target="#exampleModal<%= productIndex %>">
                        View
                    </button>
                    
                    <!-- Modal -->
                    <div class="modal fade" id="exampleModal<%= productIndex %>" tabindex="-1" aria-labelledby="exampleModalLabel<%= productIndex %>" aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h1 class="modal-title fs-5" id="exampleModalLabel<%= productIndex %>">Product Details</h1>
                                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                </div>
                                <div class="modal-body">
                                    <p><strong>Order ID:</strong> <%= orders._id %></p>
                                    <p><strong>Order Date:</strong> <%= new Date(orders.orderDate).toLocaleDateString()  %></p>
                                    <p><strong>Delivery Date:</strong> <%= new Date(orders.expectedDelivery).toLocaleDateString()  %></p>
                                    <p><strong>Total Price:</strong> <%= orders.totalAmount %></p>
                                    <p><strong>withOutDiscount:</strong> <%= orders.withoutDiscount %></p>
                                    
                                    <hr>
                                    <% if (orders.shippingAddress) { %>
                                        <h3>Shipping Address</h3>
                                        <p><strong>Name:</strong> <%= orders.shippingAddress.name %></p>
                                        <p><strong>House Number:</strong> <%= orders.shippingAddress.houseNumber %></p>
                                        <p><strong>Street:</strong> <%= orders.shippingAddress.street %></p>
                                        <p><strong>City:</strong> <%= orders.shippingAddress.city %></p>
                                        <p><strong>PIN:</strong> <%= orders.shippingAddress.pincode %></p>
                                    <% } %>
                                    <h3>Product Details</h3>
                                    <p><strong>Name:</strong> <%= product.productName %></p>
                                    <p><strong>Price:</strong> <%= product.productPrice %></p>
                                    <p><strong>Order Status:</strong><%= product.orderStatus %></p>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </td>
            </tr>
            <% }) %>
            <% }) %>
           <% }) %>
    </tbody>
</table>



    </div>
    <script src="https://cdn.jsdelivr.net/npm/axios@1.6.7/dist/axios.min.js"></script>

    
     
    <script>

function updateOrderStatus(event,orderId,productId,index){
 event.preventDefault()
 console.log(orderId,productId)
 const form=document.querySelector(`#updateOrderForm-${orderId}-${index}`)
 const status=form.querySelector('select[name="status"]').value;

 axios.post(`/updateOrderStatus/${orderId}/${productId}`,{status})
 .then(response=>{
    console.log(response.data)
    window.location.reload()
 }).catch(error=>{
    console.log(error)
 })
}





    $(document).ready(function(){
        $('#myTableCoupon').dataTable();
    });
    </script>
  

