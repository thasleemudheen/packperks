           <!--------------- product listing page start from here------------ -->
           <table id="myproductTable" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
            <thead>
              <tr>
                <th class="th-sm">Product image</th>
                <th class="th-sm">ProductId</th>
                <th class="th-sm">Name </th>
                <th class="th-sm">category</th>               
                <th class="th-sm">discription</th>
                <th class="th-sm">Price</th>
                <th class="th-sm">quantity </th>
                <th class="th-sm">Brand</th>
                <!-- <th class="th-sm"></th> -->
                
                <th class="th-sm">action</th>
                <th class="th-sm">edit</th>
              </tr>
            </thead>
            <tbody>
             
                <% product.forEach(product=> { %>
                    <tr>
                        <td class="product-image <%= product.isDisabled ? 'disabled' : '' %>">
                            <img style="height: 200px; width: 200px;" src="<%=product.productImage[0]%>" alt="productImage">
                        </td>
                      <td><%=product.productId %></td>
                      <td><%=product.productName %></td>
                      <td><%=product.categoryName %></td>
                      <td><%=product.discription %></td>
                      <td><%=product.productPrice %></td>
                      <td><%=product.stockQuantity %></td>
                      <td><%=product.brand %></td>
                      <!-- <td><%=product.createdAt%></td> -->
                       <td class="d-flex">
                        <form action="/admin/disableProduct/<%= product._id %>" class="col-6" method="post">
                            <input type="hidden" name="productId" value="<%=product._id%>">
                            <% if(product.isDisabled){%>
                                <button type="submit" class="btn btn-success">enable</button>
                           <% }else{%>
                            <button type="submit" class="btn btn-danger">disabled</button>
                           <%}%>
                            
                            
                        </form>
                    </td>
                        <td>

                        
                        <form action="/admin/editProduct/<%= product._id %>" method="get">
                            <button type="submit" class="btn btn-secondary">edit</button>
                        </form>
                      </td>
                      
               <% }) %>
              </tr>
             
          </table>
           

