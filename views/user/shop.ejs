    }
    let productHTML = '';
    productsSearch.forEach(product => {
        productHTML += `
            <div class="col-xl-4 col-md-6 col-lg-6 col-sm-6">
                <div class="product-wrap mb-25 scroll-zoom">
                    <div class="product-img">
                        <a href="/singleProduct/${product._id}">
                            <img class="default-img ${product.isDisabled ? 'disabled' : ''}" src="${product.productImage[0]}" alt="">
                            <img class="hover-img" src="${product.productImage[1]}" alt="">
                        </a>
                        ${product.isDisabled ? '<div class="error-message">Out of Stock</div>' : ''}
                        <div class="product-action">
                            <div class=""></div>
                            <div class=""></div>
                            <div class=""></div>
                        </div>
                    </div>
                    <div class="product-content text-center">
                        <h3><a href="/singleProduct/${product._id}">${product.productName}</a></h3>
                        <div class="product-price">
                            <span>₹: ${product.productPrice}</span>
                        </div>
                        <div id="wishlist_${product._id}" class="pro-details-wishlist">
                            <button onclick="toggleWishlist('${product._id}')" id="wishlist-${product._id}" style="border: none; background-color: white;">
                                ${user && user.wishlist.some(prd => prd.productId.toString() === product._id.toString()) ?
                                    '<i class="fa fa-heart fa-2x" style="color: red;"></i>' :
                                    '<i class="fa fa-heart-o fa-2x"></i>'
                                }
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        `;
    });
    productDisplay.innerHTML += productHTML;
}


