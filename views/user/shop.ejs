<script>
document.getElementById('categoryFilter').addEventListener('change', filterAndSortProducts);
document.getElementById('brandFilter').addEventListener('change', filterAndSortProducts);
document.getElementById('refineBy').addEventListener('change', filterAndSortProducts);

function filterAndSortProducts(page = 1) {
    const category = document.getElementById('categoryFilter').value;
    const brand = document.getElementById('brandFilter').value;
    const sortBy = document.getElementById('refineBy').value;

    let query = `page=${page}&`;
    if (category) query += `category=${category}&`;
    if (brand) query += `brand=${brand}&`;
    if (sortBy) query += `sortBy=${sortBy}`;

    axios.get(`/sortAndFilter?${query}`)
        .then(response => {
            const { products, user, currentPage, totalPages } = response.data;
            const productDisplay = document.getElementById('productDisplay');
            productDisplay.innerHTML = '';

            products.forEach(product => {
                const productHTML = `
                    <div class="col-xl-4 col-md-6 col-lg-6 col-sm-6 product-category" data-brand="${product.brand}" data-category="${product.categoryName}">
                      <div class="product-wrap mb-25 scroll-zoom">
                        <div class="product-img">
                          <a href="/singleProduct/${product._id}">
                            <img class="default-img ${product.isDisabled ? 'disabled' : ''}" src="${product.productImage[0]}" alt="">
                            <img class="hover-img" src="${product.productImage[1]}" alt="">
                          </a>
                          ${product.isDisabled || product.stockQuantity === 0 ? '<div class="error-message">Out of Stock</div>' : ''}
                          <div class="product-action">
                            <div class=""></div>
                            <div class=""></div>
                            <div class=""></div>
                          </div>
                        </div>
                        <div class="product-content text-center">
                          <h3><a href="/singleProduct/${product._id}">${product.productName}</a></h3>
                          <div class="product-price">
                            <span>₹: ${product.productPrice}</span>
                          </div>
                          <div id="wishlist_${product._id}" class="pro-details-wishlist">
                            <button onclick="toggleWishlist('${product._id}')" id="wishlist-${product._id}" style="border: none; background-color: white;">
                              ${user && user.wishlist.some(prd => prd.productId.toString() === product._id.toString()) ? '<i id="red_heart_${product._id}" class="fa fa-heart fa-2x" style="color: red;"></i>' : '<i id="plain_heart_${product._id}" class="fa fa-heart-o fa-2x"></i>'}
                            </button>
                          </div>
                        </div>
                      </div>
                    </div>
                `;
                productDisplay.insertAdjacentHTML('beforeend', productHTML);
            });

            // Update pagination
            const pagination = document.getElementById('pagination');
            pagination.innerHTML = `
                <li class="page-item ${currentPage === 1 ? 'disabled' : ''}">
                    <a class="page-link" href="javascript:void(0);" onclick="filterAndSortProducts(${currentPage - 1})"><i class="fa fa-angle-left"></i></a>
                </li>
                ${Array.from({ length: totalPages }, (_, i) => `
                    <li class="page-item ${currentPage === i + 1 ? 'active' : ''}">
                        <a class="page-link" href="javascript:void(0);" onclick="filterAndSortProducts(${i + 1})">${i + 1}</a>
                    </li>
                `).join('')}
                <li class="page-item ${currentPage === totalPages ? 'disabled' : ''}">
                    <a class="page-link" href="javascript:void(0);" onclick="filterAndSortProducts(${currentPage + 1})"><i class="fa fa-angle-right"></i></a>
                </li>
            `;
        })
        .catch(error => {
            console.error(error);
        });
}






    document.getElementById('searchButton').addEventListener('click',function(event){
        event.preventDefault()
        const inputValue=document.getElementById('searchInput').value;

        axios.get(`/search/${inputValue}`)
        .then(response=>{
            console.log(response);
            displayProducts(response.data)
            document.getElementById('searchInput').value;
        }).catch(error=>{
            console.error(error);
        });
    });
    function displayProducts(data) {
    const { productsSearch, user } = data;
    const productDisplay = document.getElementById('productDisplay');
    productDisplay.innerHTML = '';

    if(productsSearch.length===0){
        productDisplay.innerHTML='<h2>sorry no result found</h2>';
        return;
    }
    let productHTML = '';
    productsSearch.forEach(product => {
        productHTML += `
            <div class="col-xl-4 col-md-6 col-lg-6 col-sm-6">
                <div class="product-wrap mb-25 scroll-zoom">
                    <div class="product-img">
                        <a href="/singleProduct/${product._id}">
                            <img class="default-img ${product.isDisabled ? 'disabled' : ''}" src="${product.productImage[0]}" alt="">
                            <img class="hover-img" src="${product.productImage[1]}" alt="">
                        </a>
                        ${product.isDisabled ? '<div class="error-message">Out of Stock</div>' : ''}
                        <div class="product-action">
                            <div class=""></div>
                            <div class=""></div>
                            <div class=""></div>
                        </div>
                    </div>
                    <div class="product-content text-center">
                        <h3><a href="/singleProduct/${product._id}">${product.productName}</a></h3>
                        <div class="product-price">
                            <span>₹: ${product.productPrice}</span>
                        </div>
                        <div id="wishlist_${product._id}" class="pro-details-wishlist">
                            <button onclick="toggleWishlist('${product._id}')" id="wishlist-${product._id}" style="border: none; background-color: white;">
                                ${user && user.wishlist.some(prd => prd.productId.toString() === product._id.toString()) ?
                                    '<i class="fa fa-heart fa-2x" style="color: red;"></i>' :
                                    '<i class="fa fa-heart-o fa-2x"></i>'
                                }
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        `;
    });
    productDisplay.innerHTML += productHTML;
}


</script>

