            <script>
            const productsIn = document.querySelectorAll('.product-category');
            const itemsPerPage = 9;
            let currentPage = 1;
            let totalProducts=productsIn.length

            function updateProductRange() {
            const startIndex = (currentPage - 1) * itemsPerPage + 1;
            let endIndex = startIndex + itemsPerPage - 1;
            endIndex = endIndex > totalProducts ? totalProducts : endIndex;
            // document.getElementById('productRange').innerText = `Showing ${startIndex}-${endIndex} of ${totalProducts} result`;
        }


            function displayPage(page) {
                const startIndex = (page - 1) * itemsPerPage;
                const endIndex = startIndex + itemsPerPage;
                productsIn.forEach((product, index) => {
                    if (index >= startIndex && index < endIndex) {
                        product.style.display = 'block';
                    } else {
                        product.style.display = 'none';
                    }
                });
            }

            function setupPagination() {
                const totalPages = Math.ceil(productsIn.length / itemsPerPage);
                const paginationElement = document.getElementById('pagination');
                let html = '';

                html += `<li class="page-item"><a class="page-link prev" href="#"><i class="fa fa-angle-left"></i></a></li>`;

                for (let i = 1; i <= totalPages; i++) {
                    html += `<li class="page-item"><a class="page-link" href="#" data-page="${i}">${i}</a></li>`;
                }

                html += `<li class="page-item"><a class="page-link next" href="#"><i class="fa fa-angle-right"></i></a></li>`;

                paginationElement.innerHTML = html;

                const pageLinks = document.querySelectorAll('.page-link');
                pageLinks.forEach((link) => {
                    link.addEventListener('click', (e) => {
                        e.preventDefault();
                        if (link.classList.contains('prev')) {
                            currentPage = currentPage > 1 ? currentPage - 1 : 1;
                        } else if (link.classList.contains('next')) {
                            currentPage = currentPage < totalPages ? currentPage + 1 : totalPages;
                        } else {
                            currentPage = parseInt(e.target.getAttribute('data-page'));
                        }
                        displayPage(currentPage);
                    });
                });
            }

            // Initial setup
            updateProductRange()
            displayPage(currentPage);
            setupPagination();



              //-------------//wish list prt------------------------//
            
                function toggleWishlist(productId, user) {                                
                        // Product is already in the wishlist, so remove it
                        axios.post(`/addWishlist/${productId}`)
                            .then(response => {
                                console.log(response);
                                if (response.status == 201) {
                                    document.getElementById(`wishlist-${productId}`).innerHTML =` <i id="plain_heart_${productId}" class="fa fa-heart-o fa-2x"></i>`
                                    Toastify({
                                        text: 'Product removed from wishlist',
                                        duration: 3000,
                                        gravity: 'top',
                                        position: 'center',
                                        backgroundColor: 'red'
                                    }).showToast();
                                }
                                else if(response.status == 200){
                                    document.getElementById(`wishlist-${productId}`).innerHTML = `<i id="red_heart_${productId}" class="fa fa-heart fa-2x" style="color: red;"></i>`
                                    Toastify({
                                        text: 'Product added to wishlist',
                                        duration: 3000,
                                        gravity: 'top',
                                        position: 'center',
                                        backgroundColor: 'darkGrey'
                                    }).showToast();
                                }

                            })
                            .catch(error => {
                                console.error('Failed to remove product from wishlist:', error);
                            });
                }
                

                // filtering based on the category


                const categoryFilter = document.getElementById('categoryFilter');
                const products = document.querySelectorAll('.product-category');

                function showAllProducts() {
                    products.forEach(product => {
                        product.style.display = 'block';
                    });
                }

                categoryFilter.addEventListener('change', function() {
                    const selectedCategory = this.value;
                    products.forEach(product => {
                        const category = product.getAttribute('data-category');
                        if (selectedCategory === '' || selectedCategory === category) {
                            product.style.display = 'block';
                        } else {
                            product.style.display = 'none';
                        }
                    });
                });
