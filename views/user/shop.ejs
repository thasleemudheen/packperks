                    <script>



              //-------------//wish list prt------------------------//
            
                function toggleWishlist(productId, user) {                                
                        // Product is already in the wishlist, so remove it
                        axios.post(`/addWishlist/${productId}`)
                            .then(response => {
                                console.log(response);
                                if (response.status == 201) {
                                    document.getElementById(`wishlist-${productId}`).innerHTML =` <i id="plain_heart_${productId}" class="fa fa-heart-o fa-2x"></i>`
                                    Toastify({
                                        text: 'Product removed from wishlist',
                                        duration: 3000,
                                        gravity: 'top',
                                        position: 'center',
                                        backgroundColor: 'red'
                                    }).showToast();
                                }
                                else if(response.status == 200){
                                    document.getElementById(`wishlist-${productId}`).innerHTML = `<i id="red_heart_${productId}" class="fa fa-heart fa-2x" style="color: red;"></i>`
                                    Toastify({
                                        text: 'Product added to wishlist',
                                        duration: 3000,
                                        gravity: 'top',
                                        position: 'center',
                                        backgroundColor: 'darkGrey'
                                    }).showToast();
                                } else if(response.status === 202){
                                    Toastify({
                                        text: 'Please login to get wishlist',
                                        duration: 3000,
                                        gravity: 'top',
                                        position: 'center',
                                        backgroundColor: 'red'
                                    }).showToast();
                                }


                            })
                            .catch(error => {
                                console.error('Failed to remove product from wishlist:', error);
                            });
                }
                




                    </script>
                    
                    
                          
